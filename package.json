{
  "name": "gatsby-monorepo-sandbox",
  "version": "1.4.0",
  "description": "Gatsby project with a monorepo structure",
  "main": "index.js",
  "author": "dkyuho",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn workspace website gatsby develop --open",
    "prod": "yarn clean && yarn build && yarn serve",
    "clean": "yarn workspace website gatsby clean",
    "build": "yarn workspace website gatsby build",
    "typecheck": "tsc --noEmit",
    "typehealth": "tsc --noEmit --listFiles --diagnostics",
    "serve": "yarn workspace website gatsby serve -H 0.0.0.0 --open",
    "packageslint": "yarn eslint packages/* --ext .js,.jsx,.ts,.tsx"
  },
  "workspaces": [
    "packages/01-base/*",
    "packages/02-functional/*",
    "packages/03-brand/*",
    "packages/04-theme/*",
    "packages/05-dealership/*",
    "website"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-react": "^7.18.6",
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-slick": "^0.23.10",
    "@types/gatsbyjs__reach-router": "^2.0.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
